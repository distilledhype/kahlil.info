{
	"metadata": {
    "sitetitle": "kahlilblog"
  },
  "plugins": {
		"metalsmith-collections": {
      "articles": {
        "sortBy": "date",
        "reverse": true
      }
    },
		"metalsmith-markdown": {
      "gfm": true,
      "tables": true,
      "breaks": true
    },
		"metalsmith-permalinks": {},
    "metalsmith-register-helpers": {
      "directory": "layouts/helpers"
    },
    "metalsmith-layouts": {
      "engine": "handlebars",
      "directory": "layouts",
      "partials": "layouts/partials"
    }
  }
}
